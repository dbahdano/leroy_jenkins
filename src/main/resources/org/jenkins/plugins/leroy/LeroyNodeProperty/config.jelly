<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form" xmlns:i="jelly:fmt">
<!--<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js">-->
<!--</script>-->
    <script src="${rootURL}/plugin/leroy/myCustom.js"/>
  <!--<st:include page="js.jelly" />-->

     <!--<script src="//code.jquery.com/jquery-1.10.2.js"></script>-->

  <f:section title="Environment Settings">
    <f:block>
        <f:entry title="LEROY_HOME" field="leroyhome" >
          <f:textbox />
        </f:entry>
        <f:entry title="LEROY_CONTROLLER_PORT" field="leroycontrollerport" >
          <f:textbox value="${it.test()}" />
        </f:entry>   
<!--    <f:entry field="goalType" title="Choose Goal Type">
            <f:select />
        </f:entry>
        -->
        <f:table class="pane">
            <f:tbody>
              <tr>
                  <td class="pane-header1">Agent</td>
                  <td class="pane-header1">Environments</td>
              </tr>
              <j:forEach var="v" items="${instance.getAgentList()}">
                <tr>
                  <td class="pane">
                    ${v}
                  </td>
                  <td class="pane">
                        <!--hardcoded html tag instead of jelly tag because of html table rendering issues-->
                        <select fillurl="/jenkins/descriptorByName/org.jenkins.plugins.leroy.LeroyNodeProperty/fillEnvironmentItems" name="_.environment" value="" class="setting-input  select "><option value="test">test</option></select>
                  </td>
                </tr>
              </j:forEach>
            </f:tbody>
        </f:table>      
    </f:block>    
  </f:section>
  
</j:jelly>  